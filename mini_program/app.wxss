.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.homeBG{
  background: rgb(88,161,255);
  position: absolute;
  bottom: 0;
